{
  "name": "@antimatter-dimensions/notations",
  "version": "3.3.0",
  "description": "Notations used in Antimatter Dimensions",
  "scripts": {
    "build": "parcel build && npm run copy-minified",
    "build:ad": "parcel build --target main --target module --target ad-min && npm run copy-minified:ad",
    "build:community": "parcel build --target community --target community-module --target community-min && npm run copy-minified:community",
    "copy-minified": "npm run copy-minified:ad && npm run copy-minified:community",
    "copy-minified:ad": "node -e \"require('fs').copyFileSync('dist/ad-notations.min.js', 'docs/ad-notations.min.js')\"",
    "copy-minified:community": "node -e \"require('fs').copyFileSync('dist/community/ad-notations.community.min.js', 'docs/ad-notations.community.min.js')\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest"
  },
  "main": "dist/ad-notations.umd.js",
  "module": "dist/ad-notations.esm.mjs",
  "unpkg": "dist/ad-notations.min.js",
  "jsdelivr": "dist/ad-notations.min.js",
  "source": "src/index.ts",
  "types": "dist/types/types.d.ts",
  "ad-min": "dist/ad-notations.min.js",
  "community": "dist/community/ad-notations.community.umd.js",
  "community-module": "dist/community/ad-notations.community.esm.mjs",
  "community-min": "dist/community/ad-notations.community.min.js",
  "targets": {
    "main": {
      "source": "src/index.ts",
      "isLibrary": true,
      "outputFormat": "commonjs",
      "includeNodeModules": {
        "break_infinity.js": false,
        "tslib": false
      }
    },
    "module": {
      "source": "src/index.ts",
      "isLibrary": true,
      "outputFormat": "esmodule",
      "includeNodeModules": {
        "break_infinity.js": false,
        "tslib": false
      }
    },
    "ad-min": {
      "source": "src/index.ts",
      "isLibrary": true,
      "optimize": true,
      "outputFormat": "commonjs",
      "includeNodeModules": {
        "break_infinity.js": false,
        "tslib": false
      }
    },
    "community": {
      "source": "src/community/index.ts",
      "isLibrary": true,
      "outputFormat": "commonjs",
      "includeNodeModules": {
        "break_infinity.js": false,
        "tslib": false
      }
    },
    "community-module": {
      "source": "src/community/index.ts",
      "isLibrary": true,
      "outputFormat": "esmodule",
      "includeNodeModules": {
        "break_infinity.js": false,
        "tslib": false
      }
    },
    "community-min": {
      "source": "src/community/index.ts",
      "isLibrary": true,
      "optimize": true,
      "outputFormat": "commonjs",
      "includeNodeModules": {
        "break_infinity.js": false,
        "tslib": false
      }
    }
  },
  "keywords": [
    "notations",
    "numbers"
  ],
  "license": "MIT",
  "files": [
    "dist",
    "docs",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/antimatter-dimensions/notations.git"
  },
  "bugs": {
    "url": "https://github.com/antimatter-dimensions/notations/issues"
  },
  "homepage": "https://github.com/antimatter-dimensions/notations",
  "devDependencies": {
    "@parcel/bundler-library": "^2.16.3",
    "@parcel/packager-ts": "^2.16.3",
    "@parcel/transformer-typescript-types": "^2.16.3",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "bili": "^5.0.2",
    "eslint": "^7.0.0",
    "jest": "^26.6.3",
    "parcel": "^2.16.3",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.5.4",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "break_infinity.js": "^2.0.0",
    "tslib": "^2.8.1"
  },
  "peerDependencies": {
    "break_infinity.js": "^2.0.0"
  }
}
